		<% for(var i = 0; i < invites.length; i++) { %>
			<tr class="dynamic-td">
				<td><%= i + 1 %></td>
				<td>
					<p><span class="td-client-info">Invite From: <%= invites[i].sender %></span></p>
					<p><span class="td-client-info">Join: <%= invites[i].eventname %></span></p>
				</td>
				<td>
					<a href="/invite-accept?id=<%= invites[i]._id%>" class="btn border-shadow update">
						<span>Accept</span>
					</a>
					<a href="/invite-reject?id=<%= invites[i]._id%>" class="btn border-shadow update">
						<span>Reject</span>
					</a>
				</td>
			</tr>
		<% } %>
